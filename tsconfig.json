{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES6",
    "lib": ["ES2017", "DOM"],
    "outDir": "./js",
    "rootDir": "Frontend/Online-auction/src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "Frontend/Online-auction/src/**/*"
  ],
  "exclude": ["node_modules"]
}
